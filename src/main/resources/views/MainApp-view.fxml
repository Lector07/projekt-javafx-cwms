<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="apMain" prefHeight="480.0" prefWidth="720.0" stylesheets="@../com/project/cwmsgradle/styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.cwmsgradle.controlls.mainAppController">
    <children>
      <VBox fx:id="vbMainApp" alignment="CENTER" layoutX="188.0" layoutY="56.0" prefHeight="368.0" prefWidth="344.0">
         <children>
            <ImageView fitHeight="188.0" fitWidth="284.0">
               <image>
                  <Image url="@../icons/logo.png" />
               </image>
               <VBox.margin>
                  <Insets top="-15.0" />
               </VBox.margin>
            </ImageView>
              <Button fx:id="loginButton" alignment="CENTER" onAction="#onLoginButtonClick" prefHeight="45.0" prefWidth="150.0" text="Zaloguj">
               <VBox.margin>
                  <Insets bottom="20.0" top="5.0" />
               </VBox.margin>
            </Button>
              <Button fx:id="registerButton" onAction="#onRegisterButtonClick" prefHeight="45.0" prefWidth="150.0" stylesheets="@../com/project/cwmsgradle/styles.css" text="Zarejestruj" />
         </children>
      </VBox>
    </children>
</AnchorPane>
