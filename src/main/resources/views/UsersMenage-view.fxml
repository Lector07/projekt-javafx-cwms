<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="apUsers" prefHeight="480.0" prefWidth="720.0" stylesheets="@../com/project/cwmsgradle/styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.cwmsgradle.controlls.UsersMenageController">
   <children>
      <Label fx:id="usernameLabelUsers" layoutX="607.0" layoutY="32.0" prefHeight="32.0" prefWidth="113.0" />
      <VBox fx:id="vbUsers" alignment="TOP_CENTER" layoutX="62.0" layoutY="140.0" prefHeight="369.0" prefWidth="150.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="20.0">
         <children>
            <Button fx:id="changePasswordButton" onAction="#onEditPasswordButtonClick" mnemonicParsing="false" prefHeight="35.0" prefWidth="125.0" text="Zmiana hasła">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="deleteUserButton" onAction="#onDeleteUserButtonClick" mnemonicParsing="false" prefHeight="35.0" prefWidth="125.0" text="Usuń użytkownika">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="goBackButton" mnemonicParsing="false" onAction="#onGoBackButtonClick" prefHeight="25.0" prefWidth="100.0" text="Powrót">
               <VBox.margin>
                  <Insets top="130.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Label fx:id="usersLabel" layoutX="50.0" layoutY="32.0" prefHeight="32.0" prefWidth="126.0" text="Użytkownicy" />
      <ImageView fitHeight="32.0" fitWidth="32.0" layoutX="14.0" layoutY="32.0">
         <image>
            <Image url="@../icons/user.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="22.0" fitWidth="22.0" layoutX="576.0" layoutY="37.0">
         <image>
            <Image url="@../icons/user.png" />
         </image>
      </ImageView>
      <HBox fx:id="hbUsers" alignment="CENTER" layoutX="192.0" layoutY="81.0" prefHeight="369.0" prefWidth="510.0" AnchorPane.rightAnchor="20.0">
         <children>
            <TableView fx:id="usersTableView" prefHeight="291.0" prefWidth="492.0">
              <columns>
                <TableColumn fx:id="userIdColumn" prefWidth="47.0" text="ID" />
                <TableColumn fx:id="usernameColumn" prefWidth="103.0" text="Nazwa" />
                  <TableColumn fx:id="passwordColumn" prefWidth="102.0" text="Hasło" />
                  <TableColumn fx:id="roleColumn" prefWidth="90.0" text="Rola" />
                  <TableColumn fx:id="createdAtColumn" prefWidth="126.0" text="Utworzono" />
              </columns>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>
